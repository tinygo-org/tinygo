{
	"build-tags": ["tinygo.arm", "js", "wasm"],
	"goos": "js",
	"goarch": "wasm",
	"compiler": "clang-7",
	"gc": "marksweep",
	"linker": "arm-none-eabi-ld",
	"rtlib": "compiler-rt",
	"cflags": [
		"-Oz",
		"-mthumb",
		"-fshort-enums",
		"-fno-exceptions", "-fno-unwind-tables",
		"-ffunction-sections", "-fdata-sections"
	],
	"ldflags": [
		"--gc-sections"
	],
	"gdb": "arm-none-eabi-gdb"
}
