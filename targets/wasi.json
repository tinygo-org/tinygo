{
	"llvm-target":   "wasm32-unknown-wasi",
	"cpu":           "generic",
	"build-tags":    ["tinygo.wasm", "wasi"],
	"goos":          "linux",
	"goarch":        "arm",
	"linker":        "wasm-ld",
	"libc":          "wasi-libc",
	"scheduler":     "asyncify",
	"default-stack-size": 8192,
	"ldflags": [
		"--allow-undefined",
		"--stack-first",
		"--export-dynamic",
		"--no-demangle"
	],
	"emulator":      ["wasmtime"]
}
