{
	"llvm-target":   "wasm32-unknown-wasi",
	"cpu":           "generic",
	"features":      "+bulk-memory,+nontrapping-fptoint,+sign-ext",
	"build-tags":    ["tinygo.wasm", "wasi", "runtime_memhash_leveldb"],
	"goos":          "linux",
	"goarch":        "arm",
	"linker":        "wasm-ld",
	"libc":          "wasi-libc",
	"scheduler":     "asyncify",
	"default-stack-size": 16384,
	"cflags": [
		"-mbulk-memory",
		"-mnontrapping-fptoint",
		"-msign-ext"
	],
	"ldflags": [
		"--stack-first",
		"--no-demangle"
	],
	"extra-files": [
		"src/runtime/asm_tinygowasm.S"
	],
	"emulator":      "go run github.com/tetratelabs/wazero/cmd/wazero@v1.0.0-pre.8 run -mount=.:/ -mount={tmpDir}:/tmp {}",
	"wasm-abi":      "generic"
}
