{
	"llvm-target": "avr-unknown-unknown",
	"build-tags": ["avr", "baremetal", "linux", "arm"],
	"goos": "linux",
	"goarch": "arm",
	"compiler": "clang",
	"gc": "conservative",
	"linker": "avr-gcc",
	"scheduler": "none",
	"default-stack-size": 256,
	"cflags": [
		"--target=avr-unknown-unknown",
		"-Werror"
	],
	"ldflags": [
		"-T", "targets/avr.ld",
		"-Wl,--gc-sections"
	],
	"extra-files": [
		"src/internal/task/task_stack_avr.S",
		"src/runtime/gc_avr.S"
	],
	"gdb": ["avr-gdb"]
}
