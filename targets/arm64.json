{
	"build-tags":["arm64"],
	"goos": "linux",
	"goarch": "arm64",
	"compiler": "clang",
	"gc": "leaking",
	"scheduler": "none",
	"linker": "ld.lld",
	"rtlib": "compiler-rt",
	"libc": "picolibc",
	"tags": "arm64",
	"llvm-target": "aarch64-none-elf",
	"cflags": [
		"-Oz",
		"-Werror",
		"-fshort-enums",
		"-fomit-frame-pointer",
		"-fno-exceptions", "-fno-unwind-tables",
		"-ffunction-sections", "-fdata-sections"
	],
	"ldflags": [
		"--gc-sections"
	],
	"gdb": "gdb"
}
