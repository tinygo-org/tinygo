{
  "inherits": ["cortex-m"],
  "llvm-target": "armv7em-none-eabi",
  "build-tags": ["stm32f4disco", "stm32f407", "stm32"],
  "cflags": [
    "--target=armv7em-none-eabi",
    "-Qunused-arguments"
  ],
  "ldflags": [
    "-T", "targets/stm32f407.ld"
  ],
