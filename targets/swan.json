{
    "inherits": ["cortex-m4"],
    "build-tags": ["swan", "stm32l4r5", "stm32l4x5", "stm32l4", "stm32"],
    "serial": "uart",
    "linkerscript": "targets/stm32l4x5.ld",
    "extra-files": [
      "lib/device/stm32/stm32l4x5.s"
    ],
    "flash-method": "command",
